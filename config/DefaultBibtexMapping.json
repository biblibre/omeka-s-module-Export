{
    "title": {
        "mappings": ["dcterms:title"]
    },
    "year": {
        "type": "year",
        "mappings": ["dcterms:date"]
    },
    "author": {
        "mappings": ["dcterms:creator", "bibo:authorList"],
        "separator": " + "
    },
    "doi": {
        "mappings": ["bibo:doi"]
    },
    "edition": {
        "mappings": ["bibo:edition"]
    },
    "editor": {
        "mappings": ["bibo:editor", "bibo:editorList"]
    },
    "pages": [
        {
            "mappings": ["bibo:pages"]
        },
        {
            "type": "format",
            "format": "%s--%s",
            "mappings": ["bibo:pageStart", "bibo:pageEnd"]
        }
    ],
    "annote": {
        "mappings": ["bibo:annotates"]
    },
    "chapter": {
        "mappings": ["bibo:chapter"]
    },
    "publisher": {
        "mappings": ["dcterms:publisher"]
    },
    "type": {
        "mappings": ["dcterms:type"]
    },
    "volume": {
        "mappings": ["bibo:volume"]
    }
}
